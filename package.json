{
  "name": "audio-api-experiments",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "rm -rf ./lambda && run-p start:**",
    "start:app": "react-scripts start",
    "start:lambda": "env-cmd -f ./.env netlify-lambda serve src/lambda --config ./webpack.functions.js",
    "build": "run-p build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda --config ./webpack.functions.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --config .prettierrc --write src/**/*.{js,jsx,ts,tsx,json,css,scss,md}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@reach/router": "^1.3.4",
    "ibm-watson": "^6.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-speech-recognition": "^3.8.0",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/reach__router": "^1.3.7",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "env-cmd": "^10.1.0",
    "http-proxy-middleware": "^1.3.1",
    "netlify-lambda": "^2.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "styled-components": "^5.2.3",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
